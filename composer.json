{
    "name": "grantlucas/clockify-cli",
    "description": "CLI application for interacting with Clockify",
    "type": "project",
    "require": {
        "symfony/console": "^5.1",
        "jdecool/clockify-api": "dev-master",
        "php-http/guzzle6-adapter": "^1.0",
        "vlucas/phpdotenv": "^5.0"
    },
    "require-dev": {
        "phan/phan": "^3.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Grant Lucas",
            "email": "contact@grantlucas.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "cs": [
            "./vendor/bin/phpcs"
        ],
        "phan": [
            "./vendor/bin/phan --progress-bar --color"
        ],
        "phan-quiet": [
            "./vendor/bin/phan --no-progress-bar"
        ]
    }
}
